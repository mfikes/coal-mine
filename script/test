#!/usr/bin/env bash

checkSucceeded() {
    CMD_RESULT=$?
    if [ $CMD_RESULT != 0 ]; then
        exit $CMD_RESULT
    fi
}

script/test-pre
checkSucceeded
script/test-part 1
checkSucceeded
script/test-part 2
checkSucceeded
script/test-part 3
checkSucceeded
script/test-part 4
checkSucceeded
script/test-part 5
checkSucceeded
script/test-post
